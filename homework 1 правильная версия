import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner scanner = new Scanner(System.in);
        System.out.print("Ввод");
        int i = scanner.nextInt();
        String s="";
        while (i>0) {
        i=i/2;
        s=s+(i%2);
        }
		System.out.println(s);
	}
}
